{
  "name": "knockout-arraytransforms",
  "version": "1.0.0",
  "description": "Efficient transformations of observable arrays (sortBy, map, filter/reject, groupBy, any/some, all/every).",
  "keywords": [
    "knockout",
    "observableArray"
  ],
  "homepage": "https://github.com/mwiencek/knockout-arraytransforms",
  "bugs": "https://github.com/mwiencek/knockout-arraytransforms/issues",
  "license": "X11",
  "author": "Michael Wiencek <mwiencek1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/mwiencek/knockout-arraytransforms.git"
  },
  "dependencies": {
    "knockout": "^3.1.0"
  },
  "devDependencies": {
    "browserify": "^10.2.6",
    "tape": "^3.5.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "chrome/24..latest",
      "firefox/17..latest",
      "safari/5..latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "scripts": {
    "test": "./node_modules/.bin/tape test/*.js",
    "build": "./node_modules/.bin/browserify index.js -x knockout -o dist/knockout-arraytransforms.js"
  }
}
